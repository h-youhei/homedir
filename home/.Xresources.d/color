#define BLACK #000000
#define WHITE #ffffff
#define RED #ff6666
#define GREEN #66FF66
#define YELLOW #dddd11
#define BLUE #6666ff
#define MAGENTA #dd11dd
#define CYAN #11dddd
#define BLACKGRAY #333333
#define WHITEGRAY #666666

*background: BLACK
*foreground: WHITE

*color0: BLACKGRAY
*color8: BLACKGRAY

*color1: RED
*color9: RED

*color2: GREEN
*color10: GREEN

*color3: YELLOW
*color11: YELLOW

*color4: BLUE
*color12: BLUE

*color5: MAGENTA
*color13: MAGENTA

*color6: CYAN
*color14: CYAN

*color7: WHITEGRAY
*color15: WHITEGRAY


!xterm*highlightColorMode: true
xterm*highlightColor: WHITEGRAY
!xterm*highlightTextColor: BLACK

! bold
xterm*colorBD: WHITE

! italic
xterm*colorIT: WHITE
! under line
xterm*colorUL: WHITE
xterm*cursorColor: WHITE
xterm*pointerColor: WHITE
xterm*pointerColorBackground: BLACK

! vim:ft=xdefaults
